{
  "private": true,
  "name": "vitau",
  "description": "Vitau UI Library",
  "license": "MIT",
  "author": "Daniel Sepulveda",
  "version": "0.1.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "boot": "yarn prestart && yarn build",
    "bootstrap": "lerna bootstrap --use-workspaces",
    "build": "lerna run build --stream",
    "check:pkgs": "manypkg check",
    "clean": "lerna clean --yes && rimraf node_modules",
    "commit": "git-cz",
    "contributors:generate": "all-contributors generate",
    "fix:pkgs": "manypkg fix",
    "gen:pkg": "ts-node utils/scripts/plopfile.ts",
    "postinstall": "yarn bootstrap && yarn check:pkgs",
    "lint": "lerna run lint --stream",
    "publish:pkgs": "node utils/scripts/publish.js",
    "release": "node utils/scripts/release.js",
    "prestart": "yarn && yarn bootstrap",
    "start": "yarn build && yarn storybook",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "test": "jest --passWithNoTests --watch"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@auto-it/all-contributors": "^10.21.0",
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.16",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@manypkg/cli": "^0.17.0",
    "@storybook/addon-a11y": "^6.1.18",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.1",
    "@types/clear": "^0.1.1",
    "@types/figlet": "^1.2.1",
    "@types/jest": "^26.0.20",
    "@types/jest-axe": "^3.5.1",
    "@types/lodash-es": "^4.17.4",
    "@types/react": "^17.0.2",
    "@types/shelljs": "^0.8.8",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "all-contributors-cli": "^6.20.0",
    "auto": "^10.21.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "babel-plugin-styled-components": "^1.12.0",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "commitizen": "^4.2.2",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "execa": "^5.0.0",
    "figlet": "^1.5.0",
    "glob-to-regexp": "^0.4.1",
    "husky": "^4.3.0",
    "inquirer": "^8.0.0",
    "jest": "^26.6.3",
    "jest-axe": "^4.1.0",
    "jest-haste-map": "^26.6.2",
    "jest-resolve": "^26.6.2",
    "jest-styled-components": "^7.0.3",
    "jest-watch-typeahead": "^0.6.1",
    "jest-watch-yarn-workspaces": "^1.1.0",
    "lerna": "^3.22.1",
    "lerna-changelog": "^1.0.1",
    "lint-staged": "^10.5.1",
    "lodash-es": "^4.17.20",
    "node-plop": "^0.26.2",
    "nodemon": "^2.0.7",
    "ora": "^5.3.0",
    "prettier": "^2.1.2",
    "prettier-package-json": "^2.1.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "styled-components": "^5.2.1",
    "stylelint": "^13.10.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "temp": "^0.9.4",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  }
}
